
<?xml version="1.0"?>
<root main_tree_to_execute="MainTree">

    <BehaviorTree ID="MainTree">
        <Sequence>
            <Action ID="GoToDoor"/>
            <Action ID="CountPeople" person_count="{person_count}"/>
            <Script code='std::cout << "People counted: " << person_count << std::endl;'/>
        </Sequence>
    </BehaviorTree>

    <TreeNodesModel>
        <Action ID="CountPeople">
            <output_port name="person_count"/>
        </Action>
        <Action ID="GoToDoor"/>
    </TreeNodesModel>

</root>
