<?xml version="1.0"?>
<root main_tree_to_execute="MainTree">

    <BehaviorTree ID="MainTree">
        <Sequence>
            <Action ID="GoToDoor"/>
            <Action ID="CountPeople" personas="{personas}"/>
        </Sequence>
    </BehaviorTree>

    <TreeNodesModel>
        <Action ID="CountPeople">
            <output_port name="personas"/>
        </Action>
        <Action ID="GoToDoor"/>
    </TreeNodesModel>

</root>
